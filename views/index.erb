<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>Company Name</th>
      <th>Phone</th>
      <th>Website</th>
    </tr>

    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.name %></td>
        <td><%= account.phone %></td>
        <td><%= account.website %></td>
      </tr>
    <% end %>
  </table>

</div>
